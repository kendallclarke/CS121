import java.util.Scanner;

public class BadSubscriptCaught {
    public static void main(String[] args) {
        // array of first names
        String[] firstNames = {
                "Alice", "Molly", "Charles", "David", "Eva",
                "James", "Sam", "Mia", "Tom", "Julia"
        };

        Scanner scanner = new Scanner(System.in);

        try {
            System.out.println("Enter an index (0 to 9): ");
            int index = scanner.nextInt();

            // Display the name at the requested position
            System.out.println("Name at index " + index + ": " + firstNames[index]);
        } catch (ArrayIndexOutOfBoundsException e) {
            System.out.println("Error: Index is out of range. Please enter a valid index (0 to 9).");
        }

        scanner.close();
    }
}
